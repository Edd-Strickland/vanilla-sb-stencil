{
  "name": "vanilla-demo",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "scripts": {
    "build:all": "rm -rf www && rm -rf public && npm run storybook:build",
    "dsm": "dsm-storybook publish -s www -c .storybook",
    "storybook:build": "build-storybook -s www -c .storybook -o public",
    "poststorybook:build": "node static/post-build.js",
    "storybook:build:all": "rm -rf www && rm -rf public && npm run storybook:build && npm run storybook:serve",
    "storybook": "start-storybook -p 7002 -s www -c .storybook",
    "storybook:serve": "http-server -c-1 public -o",
    "reset:modules": "rm -rf node_modules && npm install"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@invisionapp/dsm-storybook": "0.0.104",
    "@storybook/addon-options": "5.2.4",
    "@storybook/html": "5.2.4",
    "babel-loader": "8.0.6",
    "fs-extra": "8.1.0",
    "npm-run-all": "4.1.5",
    "path": "0.12.7"
  },
  "license": "MIT",
  "dependencies": {
    "request": "2.88.0"
  }
}
